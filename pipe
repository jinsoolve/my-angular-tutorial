#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {
    vector<string> names;
    string line;

    // 표준 입력에서 이름 리스트 받기 (엔터로 구분)
    while (getline(cin, line)) {
        if (line.empty()) continue;
        names.push_back(line);
    }

    cout << "[\n";
    for (size_t i = 0; i < names.size(); ++i) {
        const string& name = names[i];
        cout << "  {\n";
        cout << "    \"id\": \"" << name << "\",\n";
        cout << "    \"name\": \"" << name << "\",\n";
        cout << "    \"route\": \"/" << name << "\"\n";
        cout << "  }" << (i + 1 < names.size() ? "," : "") << "\n";
    }
    cout << "]\n";

    return 0;
}